{
    "name": "asimnet/notify",
    "version": "1.3.0",
    "description": "Laravel notification center with FCM, multi-channel support, and Filament admin",
    "type": "library",
    "keywords": [
        "laravel",
        "notifications",
        "fcm",
        "firebase",
        "push-notifications",
        "filament",
        "multi-tenant"
    ],
    "homepage": "https://github.com/asimnet/notify",
    "license": "proprietary",
    "authors": [
        {
            "name": "Asimnet",
            "email": "dev@asimnet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/notifications": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "kreait/firebase-php": "^7.0",
        "spatie/laravel-package-tools": "^1.92",
        "spatie/laravel-settings": "^3.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Asimnet\\Notify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asimnet\\Notify\\Tests\\": "tests/",
            "Asimnet\\Notify\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asimnet\\Notify\\NotifyServiceProvider"
            ],
            "aliases": {
                "Notify": "Asimnet\\Notify\\Facades\\Notify"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
